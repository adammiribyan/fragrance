<%= simple_form_for(@item) do |f| %>
  <%= f.error_notification %>

  <div class="inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :gender %>
    <%= f.input :volume %>
    <%= f.input :launched_at %>
  </div>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
